{
	// Place your snippets for properties here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"linguist detectable": {
		"prefix": "lde",
		"body": [
			"${1:linguist-detectable}"
		]
	},
	"linguist documentation": {
		"prefix": "ldo",
		"body": [
			"${1:linguist-documentation}"
		]
	},
	"linguist generated": {
		"prefix": "lge",
		"body": [
			"${1:linguist-generated}"
		]
	},
	"linguist language": {
		"prefix": "lla",
		"body": [
			"${1:linguist-language=${0:name}}"
		]
	},
	"linguist vendored": {
		"prefix": "lve",
		"body": [
			"${0:linguist-vendored}"
		]
	},
}